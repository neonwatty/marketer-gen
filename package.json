{
  "dependencies": {
    "rails_admin": "3.3.0",
    "sass": "^1.89.2",
    "reactflow": "^11.11.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.4.7",
    "immer": "^10.0.3",
    "uuid": "^9.0.1",
    "lodash.debounce": "^4.0.8",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build:css": "sass ./app/assets/stylesheets/application.sass.scss:./app/assets/builds/application.css ./app/assets/stylesheets/rails_admin.scss:./app/assets/builds/rails_admin.css --no-source-map --load-path=node_modules",
    "build:js": "esbuild app/javascript/*.tsx app/javascript/*.ts --bundle --sourcemap --outdir=app/assets/builds --public-path=/assets --loader:.tsx=tsx --loader:.ts=ts",
    "build": "npm run build:css && npm run build:js"
  },
  "devDependencies": {
    "esbuild": "^0.19.8"
  }
}
