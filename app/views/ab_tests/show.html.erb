<% content_for :title, "#{@ab_test.name} - A/B Test Details" %>

<div class="min-h-screen bg-gradient-to-br from-slate-50 to-slate-100">
  <!-- Header Section -->
  <div class="bg-white border-b border-gray-200 shadow-sm">
    <div class="px-4 sm:px-6 lg:px-8 py-6">
      <div class="flex items-center justify-between">
        <div class="flex-1 min-w-0">
          <!-- Breadcrumb -->
          <nav class="flex mb-4" aria-label="Breadcrumb">
            <ol class="inline-flex items-center space-x-1 md:space-x-3">
              <li class="inline-flex items-center">
                <%= link_to ab_tests_path, class: "inline-flex items-center text-sm font-medium text-gray-700 hover:text-blue-600" do %>
                  <svg class="w-4 h-4 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z"></path>
                  </svg>
                  A/B Tests
                <% end %>
              </li>
              <li>
                <div class="flex items-center">
                  <svg class="w-6 h-6 text-gray-400" fill="currentColor" viewBox="0 0 20 20">
                    <path fill-rule="evenodd" d="M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z" clip-rule="evenodd"></path>
                  </svg>
                  <span class="ml-1 text-sm font-medium text-gray-500 md:ml-2"><%= @ab_test.name %></span>
                </div>
              </li>
            </ol>
          </nav>

          <div class="flex items-center space-x-4">
            <h1 class="text-heading-h2 text-gray-900 font-bold"><%= @ab_test.name %></h1>
            
            <!-- Status Badge -->
            <% case @ab_test.status %>
            <% when 'running' %>
              <span class="inline-flex items-center px-3 py-1 rounded-full text-sm font-medium bg-green-100 text-green-800">
                <svg class="-ml-0.5 mr-1.5 h-2 w-2 text-green-400" fill="currentColor" viewBox="0 0 8 8">
                  <circle cx="4" cy="4" r="3" />
                </svg>
                Running
              </span>
            <% when 'paused' %>
              <span class="inline-flex items-center px-3 py-1 rounded-full text-sm font-medium bg-yellow-100 text-yellow-800">
                <svg class="-ml-0.5 mr-1.5 h-2 w-2 text-yellow-400" fill="currentColor" viewBox="0 0 8 8">
                  <circle cx="4" cy="4" r="3" />
                </svg>
                Paused
              </span>
            <% when 'completed' %>
              <span class="inline-flex items-center px-3 py-1 rounded-full text-sm font-medium bg-blue-100 text-blue-800">
                <svg class="-ml-0.5 mr-1.5 h-2 w-2 text-blue-400" fill="currentColor" viewBox="0 0 8 8">
                  <circle cx="4" cy="4" r="3" />
                </svg>
                Completed
              </span>
            <% when 'draft' %>
              <span class="inline-flex items-center px-3 py-1 rounded-full text-sm font-medium bg-gray-100 text-gray-800">
                <svg class="-ml-0.5 mr-1.5 h-2 w-2 text-gray-400" fill="currentColor" viewBox="0 0 8 8">
                  <circle cx="4" cy="4" r="3" />
                </svg>
                Draft
              </span>
            <% end %>

            <!-- Winner Badge -->
            <% if @ab_test.winner_declared? %>
              <span class="inline-flex items-center px-3 py-1 rounded-full text-sm font-medium bg-amber-100 text-amber-800">
                <svg class="-ml-0.5 mr-1.5 h-2 w-2 text-amber-400" fill="currentColor" viewBox="0 0 8 8">
                  <circle cx="4" cy="4" r="3" />
                </svg>
                Winner: <%= @ab_test.winner_variant.name %>
              </span>
            <% end %>
          </div>
          
          <p class="mt-2 text-secondary">
            <%= @ab_test.description.present? ? @ab_test.description : "#{@ab_test.test_type.humanize} test" %>
          </p>
        </div>
        
        <!-- Action Buttons -->
        <div class="flex items-center space-x-3" data-controller="loading">
          <% if @ab_test.draft? %>
            <%= link_to start_ab_test_path(@ab_test), method: :post,
                        class: "inline-flex items-center px-4 py-2 bg-green-600 text-white text-sm font-medium rounded-lg hover:bg-green-700 focus:outline-none focus:ring-2 focus:ring-green-500 focus:ring-offset-2 transition-colors duration-200",
                        data: { action: "click->loading#show", loading_text: "Starting..." } do %>
              <svg class="w-5 h-5 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M14.828 14.828a4 4 0 01-5.656 0M9 10h1m4 0h1m-6 4h8m-6 4h8m-6 4h8M7 7h.01M7 3h5.99"></path>
              </svg>
              Start Test
            <% end %>
          <% elsif @ab_test.running? %>
            <%= link_to pause_ab_test_path(@ab_test), method: :post,
                        class: "inline-flex items-center px-4 py-2 bg-yellow-600 text-white text-sm font-medium rounded-lg hover:bg-yellow-700 focus:outline-none focus:ring-2 focus:ring-yellow-500 focus:ring-offset-2 transition-colors duration-200",
                        data: { action: "click->loading#show", loading_text: "Pausing..." } do %>
              <svg class="w-5 h-5 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 9v6m4-6v6m7-3a9 9 0 11-18 0 9 9 0 0118 0z"></path>
              </svg>
              Pause Test
            <% end %>
          <% elsif @ab_test.paused? %>
            <%= link_to resume_ab_test_path(@ab_test), method: :post,
                        class: "inline-flex items-center px-4 py-2 bg-green-600 text-white text-sm font-medium rounded-lg hover:bg-green-700 focus:outline-none focus:ring-2 focus:ring-green-500 focus:ring-offset-2 transition-colors duration-200",
                        data: { action: "click->loading#show", loading_text: "Resuming..." } do %>
              <svg class="w-5 h-5 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M14.828 14.828a4 4 0 01-5.656 0M9 10h1m4 0h1m-6 4h8m-6 4h8m-6 4h8M7 7h.01M7 3h5.99"></path>
              </svg>
              Resume Test
            <% end %>
          <% end %>

          <% if @ab_test.running? && @ab_test.can_complete? %>
            <%= link_to complete_ab_test_path(@ab_test), method: :post,
                        class: "inline-flex items-center px-4 py-2 bg-blue-600 text-white text-sm font-medium rounded-lg hover:bg-blue-700 focus:outline-none focus:ring-2 focus:ring-blue-500 focus:ring-offset-2 transition-colors duration-200",
                        data: { action: "click->loading#show", loading_text: "Completing...", confirm: "Complete this test and declare a winner based on current results?" } do %>
              <svg class="w-5 h-5 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4M7.835 4.697a3.42 3.42 0 001.946-.806 3.42 3.42 0 014.438 0 3.42 3.42 0 001.946.806 3.42 3.42 0 013.138 3.138 3.42 3.42 0 00.806 1.946 3.42 3.42 0 010 4.438 3.42 3.42 0 00-.806 1.946 3.42 3.42 0 01-3.138 3.138 3.42 3.42 0 00-1.946.806 3.42 3.42 0 01-4.438 0 3.42 3.42 0 00-1.946-.806 3.42 3.42 0 01-3.138-3.138 3.42 3.42 0 00-.806-1.946 3.42 3.42 0 010-4.438 3.42 3.42 0 00.806-1.946 3.42 3.42 0 013.138-3.138z"></path>
              </svg>
              Complete Test
            <% end %>
          <% end %>

          <%= link_to edit_ab_test_path(@ab_test),
                      class: "inline-flex items-center px-4 py-2 border border-gray-300 text-sm font-medium rounded-lg text-gray-700 bg-white hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-blue-500 focus:ring-offset-2 transition-colors duration-200" do %>
            <svg class="w-5 h-5 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M11 5H6a2 2 0 00-2 2v11a2 2 0 002 2h11a2 2 0 002-2v-5m-1.414-9.414a2 2 0 112.828 2.828L11.828 15H9v-2.828l8.586-8.586z"></path>
            </svg>
            Edit
          <% end %>

          <%= link_to results_ab_test_path(@ab_test),
                      class: "inline-flex items-center px-4 py-2 border border-gray-300 text-sm font-medium rounded-lg text-gray-700 bg-white hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-blue-500 focus:ring-offset-2 transition-colors duration-200" do %>
            <svg class="w-5 h-5 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z"></path>
            </svg>
            View Results
          <% end %>
        </div>
      </div>
    </div>
  </div>

  <!-- Main Content -->
  <div class="px-4 sm:px-6 lg:px-8 py-8" 
       data-controller="ab-test-monitor status-poller"
       data-ab-test-monitor-test-id-value="<%= @ab_test.id %>"
       data-status-poller-url-value="<%= live_metrics_ab_test_path(@ab_test) %>"
       data-status-poller-interval-value="10000">
    
    <!-- Progress and Timeline (for running/paused tests) -->
    <% if @ab_test.running? || @ab_test.paused? %>
      <div class="bg-white rounded-xl border border-gray-200 shadow-sm p-6 mb-8">
        <div class="flex items-center justify-between mb-4">
          <h2 class="text-lg font-semibold text-gray-900">Test Progress</h2>
          <div class="text-sm text-gray-500">
            <% if @ab_test.end_date %>
              <%= pluralize((@ab_test.end_date - Date.current).to_i, 'day') %> remaining
            <% else %>
              No end date set
            <% end %>
          </div>
        </div>
        
        <div class="mb-4">
          <div class="flex justify-between text-sm text-gray-600 mb-2">
            <span>Duration Progress</span>
            <span><%= @ab_test.progress_percentage %>% complete</span>
          </div>
          <div class="w-full bg-gray-200 rounded-full h-3">
            <div class="bg-blue-600 h-3 rounded-full transition-all duration-300" 
                 style="width: <%= @ab_test.progress_percentage %>%"
                 data-ab-test-monitor-target="progressBar"></div>
          </div>
        </div>

        <div class="grid grid-cols-1 md:grid-cols-4 gap-4">
          <div class="text-center p-3 bg-gray-50 rounded-lg">
            <div class="text-sm font-medium text-gray-500">Days Running</div>
            <div class="text-xl font-bold text-gray-900" data-ab-test-monitor-target="daysRunning">
              <%= @ab_test.duration_days.to_i %>
            </div>
          </div>
          
          <div class="text-center p-3 bg-gray-50 rounded-lg">
            <div class="text-sm font-medium text-gray-500">Total Visitors</div>
            <div class="text-xl font-bold text-gray-900" data-ab-test-monitor-target="totalVisitors">
              <%= number_with_delimiter(@performance_data[:variants].sum { |v| v[:total_visitors] }) %>
            </div>
          </div>
          
          <div class="text-center p-3 bg-gray-50 rounded-lg">
            <div class="text-sm font-medium text-gray-500">Total Conversions</div>
            <div class="text-xl font-bold text-gray-900" data-ab-test-monitor-target="totalConversions">
              <%= number_with_delimiter(@performance_data[:variants].sum { |v| v[:conversions] }) %>
            </div>
          </div>
          
          <div class="text-center p-3 bg-gray-50 rounded-lg">
            <div class="text-sm font-medium text-gray-500">Confidence Level</div>
            <div class="text-xl font-bold <%= @ab_test.statistical_significance_reached? ? 'text-green-600' : 'text-gray-900' %>" 
                 data-ab-test-monitor-target="confidenceLevel">
              <% if @ab_test.statistical_significance_reached? %>
                <%= @ab_test.confidence_level %>%
              <% else %>
                <span class="text-gray-500">--</span>
              <% end %>
            </div>
          </div>
        </div>
      </div>
    <% end %>

    <!-- Key Metrics Grid -->
    <div class="grid grid-cols-1 lg:grid-cols-3 gap-8 mb-8">
      <!-- Overall Performance -->
      <div class="lg:col-span-2">
        <div class="bg-white rounded-xl border border-gray-200 shadow-sm p-6">
          <h2 class="text-lg font-semibold text-gray-900 mb-6">Variant Performance</h2>
          
          <div class="space-y-4">
            <% @performance_data[:variants].each do |variant| %>
              <div class="border border-gray-200 rounded-lg p-4">
                <div class="flex items-center justify-between mb-3">
                  <div class="flex items-center space-x-3">
                    <h3 class="font-medium text-gray-900"><%= variant[:name] %></h3>
                    <% if variant[:is_control] %>
                      <span class="inline-flex items-center px-2 py-1 rounded text-xs font-medium bg-blue-100 text-blue-800">
                        Control
                      </span>
                    <% else %>
                      <span class="inline-flex items-center px-2 py-1 rounded text-xs font-medium bg-purple-100 text-purple-800">
                        Treatment
                      </span>
                    <% end %>
                    
                    <% if @ab_test.winner_variant_id == variant[:id] %>
                      <span class="inline-flex items-center px-2 py-1 rounded text-xs font-medium bg-amber-100 text-amber-800">
                        <svg class="w-3 h-3 mr-1" fill="currentColor" viewBox="0 0 20 20">
                          <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
                        </svg>
                        Winner
                      </span>
                    <% end %>
                  </div>
                  
                  <div class="text-sm text-gray-500">
                    <%= variant[:traffic_percentage].to_i %>% traffic
                  </div>
                </div>

                <div class="grid grid-cols-1 md:grid-cols-4 gap-4">
                  <div class="text-center p-3 bg-gray-50 rounded">
                    <div class="text-sm font-medium text-gray-500">Visitors</div>
                    <div class="text-lg font-bold text-gray-900">
                      <%= number_with_delimiter(variant[:total_visitors]) %>
                    </div>
                  </div>
                  
                  <div class="text-center p-3 bg-gray-50 rounded">
                    <div class="text-sm font-medium text-gray-500">Conversions</div>
                    <div class="text-lg font-bold text-gray-900">
                      <%= number_with_delimiter(variant[:conversions]) %>
                    </div>
                  </div>
                  
                  <div class="text-center p-3 bg-gray-50 rounded">
                    <div class="text-sm font-medium text-gray-500">Conv. Rate</div>
                    <div class="text-lg font-bold text-gray-900">
                      <%= number_to_percentage(variant[:conversion_rate], precision: 1) %>
                    </div>
                  </div>
                  
                  <% unless variant[:is_control] %>
                    <div class="text-center p-3 bg-gray-50 rounded">
                      <div class="text-sm font-medium text-gray-500">Lift</div>
                      <% lift = @variant_comparisons.find { |v| v[:variant_name] == variant[:name] }&.dig(:lift_percentage) || 0 %>
                      <div class="text-lg font-bold <%= lift > 0 ? 'text-green-600' : lift < 0 ? 'text-red-600' : 'text-gray-900' %>">
                        <% if lift > 0 %>+<% end %><%= number_to_percentage(lift, precision: 1) %>
                      </div>
                    </div>
                  <% else %>
                    <div class="text-center p-3 bg-gray-50 rounded">
                      <div class="text-sm font-medium text-gray-500">Baseline</div>
                      <div class="text-lg font-bold text-gray-900">--</div>
                    </div>
                  <% end %>
                </div>
              </div>
            <% end %>
          </div>
        </div>
      </div>

      <!-- Statistical Analysis -->
      <div class="space-y-6">
        <!-- Statistical Significance -->
        <div class="bg-white rounded-xl border border-gray-200 shadow-sm p-6">
          <h3 class="text-lg font-semibold text-gray-900 mb-4">Statistical Analysis</h3>
          
          <div class="space-y-4">
            <% if @statistical_analysis.present? %>
              <div class="flex items-center justify-between p-3 rounded-lg <%= @statistical_analysis[:is_significant] ? 'bg-green-50' : 'bg-yellow-50' %>">
                <div>
                  <div class="text-sm font-medium <%= @statistical_analysis[:is_significant] ? 'text-green-800' : 'text-yellow-800' %>">
                    Statistical Significance
                  </div>
                  <div class="text-xs <%= @statistical_analysis[:is_significant] ? 'text-green-600' : 'text-yellow-600' %>">
                    p-value: <%= @statistical_analysis[:p_value] %>
                  </div>
                </div>
                <div class="text-right">
                  <div class="text-lg font-bold <%= @statistical_analysis[:is_significant] ? 'text-green-800' : 'text-yellow-800' %>">
                    <%= number_to_percentage(@statistical_analysis[:confidence_interval], precision: 1) %>
                  </div>
                </div>
              </div>
            <% else %>
              <div class="flex items-center justify-between p-3 rounded-lg bg-gray-50">
                <div>
                  <div class="text-sm font-medium text-gray-800">Statistical Significance</div>
                  <div class="text-xs text-gray-600">Insufficient data</div>
                </div>
                <div class="text-right">
                  <div class="text-lg font-bold text-gray-800">--</div>
                </div>
              </div>
            <% end %>

            <div class="border-t border-gray-200 pt-4">
              <h4 class="text-sm font-medium text-gray-900 mb-3">Test Configuration</h4>
              <div class="space-y-2 text-sm">
                <div class="flex justify-between">
                  <span class="text-gray-500">Test Type:</span>
                  <span class="font-medium text-gray-900"><%= @ab_test.test_type.humanize %></span>
                </div>
                <div class="flex justify-between">
                  <span class="text-gray-500">Confidence Level:</span>
                  <span class="font-medium text-gray-900"><%= @ab_test.confidence_level %>%</span>
                </div>
                <div class="flex justify-between">
                  <span class="text-gray-500">Significance Threshold:</span>
                  <span class="font-medium text-gray-900"><%= @ab_test.significance_threshold %>%</span>
                </div>
                <% if @ab_test.minimum_sample_size.present? %>
                  <div class="flex justify-between">
                    <span class="text-gray-500">Min Sample Size:</span>
                    <span class="font-medium text-gray-900"><%= number_with_delimiter(@ab_test.minimum_sample_size) %></span>
                  </div>
                <% end %>
              </div>
            </div>
          </div>
        </div>

        <!-- Recommendations -->
        <div class="bg-white rounded-xl border border-gray-200 shadow-sm p-6">
          <h3 class="text-lg font-semibold text-gray-900 mb-4">Recommendations</h3>
          
          <div class="space-y-3">
            <% if @insights.present? && @insights[:recommendations].any? %>
              <% @insights[:recommendations].each do |recommendation| %>
                <div class="flex items-start space-x-3 p-3 bg-blue-50 rounded-lg">
                  <svg class="w-5 h-5 text-blue-500 mt-0.5 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 16h-1v-4h-1m1-4h.01M21 12a9 9 0 11-18 0 9 9 0 0118 0z"></path>
                  </svg>
                  <div class="text-sm text-blue-800">
                    <%= recommendation %>
                  </div>
                </div>
              <% end %>
            <% else %>
              <div class="text-center py-4">
                <svg class="mx-auto h-8 w-8 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9.663 17h4.673M12 3v1m6.364 1.636l-.707.707M21 12h-1M4 12H3m3.343-5.657l-.707-.707m2.828 9.9a5 5 0 117.072 0l-.548.547A3.374 3.374 0 0014 18.469V19a2 2 0 11-4 0v-.531c0-.895-.356-1.754-.988-2.386l-.548-.547z"></path>
                </svg>
                <p class="mt-2 text-sm text-gray-500">No recommendations available yet</p>
              </div>
            <% end %>
          </div>
          
          <% if @insights.present? && @insights[:next_steps].any? %>
            <div class="mt-4 pt-4 border-t border-gray-200">
              <h4 class="text-sm font-medium text-gray-900 mb-2">Next Steps</h4>
              <ul class="space-y-1">
                <% @insights[:next_steps].each do |step| %>
                  <li class="flex items-center text-sm text-gray-600">
                    <svg class="w-4 h-4 mr-2 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"></path>
                    </svg>
                    <%= step %>
                  </li>
                <% end %>
              </ul>
            </div>
          <% end %>
        </div>
      </div>
    </div>

    <!-- Test Information -->
    <div class="bg-white rounded-xl border border-gray-200 shadow-sm p-6">
      <h2 class="text-lg font-semibold text-gray-900 mb-6">Test Information</h2>
      
      <div class="grid grid-cols-1 md:grid-cols-2 gap-6">
        <div>
          <h3 class="text-sm font-medium text-gray-900 mb-3">Test Details</h3>
          <dl class="space-y-2">
            <div class="flex justify-between">
              <dt class="text-sm text-gray-500">Campaign:</dt>
              <dd class="text-sm font-medium text-gray-900">
                <%= link_to @ab_test.campaign.name, campaign_path(@ab_test.campaign), 
                            class: "text-blue-600 hover:text-blue-800" %>
              </dd>
            </div>
            <div class="flex justify-between">
              <dt class="text-sm text-gray-500">Created:</dt>
              <dd class="text-sm font-medium text-gray-900">
                <%= @ab_test.created_at.strftime("%B %d, %Y at %l:%M %p") %>
              </dd>
            </div>
            <% if @ab_test.start_date %>
              <div class="flex justify-between">
                <dt class="text-sm text-gray-500">Started:</dt>
                <dd class="text-sm font-medium text-gray-900">
                  <%= @ab_test.start_date.strftime("%B %d, %Y at %l:%M %p") %>
                </dd>
              </div>
            <% end %>
            <% if @ab_test.end_date %>
              <div class="flex justify-between">
                <dt class="text-sm text-gray-500">
                  <%= @ab_test.completed? ? 'Completed:' : 'Scheduled End:' %>
                </dt>
                <dd class="text-sm font-medium text-gray-900">
                  <%= @ab_test.end_date.strftime("%B %d, %Y at %l:%M %p") %>
                </dd>
              </div>
            <% end %>
          </dl>
        </div>

